{
  "name": "yeet-bot-v2",
  "module": "src/index.ts",
  "type": "module",
  "version": "2.0.0",
  "description": "Modular rule-based Yeet bot for Berachain with comprehensive testing",
  "main": "src/index.ts",
  "scripts": {
    "start": "bun run src/scripts/start.ts",
    "start:dry": "DRY_RUN=true bun run src/scripts/start.ts",
    "start:dashboard": "bun run src/scripts/start-dashboard.ts",
    "start:dashboard:dry": "DRY_RUN=true bun run src/scripts/start-dashboard.ts",
    "start:web": "bun run src/scripts/start-web.ts",
    "start:web:dry": "DRY_RUN=true bun run src/scripts/start-web.ts",
    "start:all": "bun run src/scripts/start-all.ts",
    "start:all:dry": "DRY_RUN=true bun run src/scripts/start-all.ts",
    "web:dev": "cd web && bun run dev",
    "web:build": "cd web && bun run build",
    "dev": "bun run --watch src/scripts/start.ts",
    "claim": "bun run src/scripts/claim-bgt.ts",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "type-check": "tsc --noEmit",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "build:check": "bun run type-check && bun run lint"
  },
  "devDependencies": {
    "@types/node": "^24.0.14",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.37.0",
    "@vitest/coverage-v8": "^2.1.8",
    "@vitest/ui": "^2.1.8",
    "bun-types": "latest",
    "eslint": "^9.31.0",
    "vitest": "^2.1.8"
  },
  "peerDependencies": {
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@aws-sdk/client-cloudwatch": "^3.848.0",
    "@aws-sdk/client-sqs": "^3.0.0",
    "@types/blessed": "^0.1.25",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/react": "^19.1.10",
    "@types/terminal-kit": "^2.5.7",
    "blessed": "^0.1.81",
    "cors": "^2.8.5",
    "dotenv": "^17.2.0",
    "express": "^4.18.2",
    "ink": "^6.2.1",
    "ink-select-input": "^6.2.0",
    "ink-spinner": "^5.0.0",
    "ink-text-input": "^6.0.0",
    "react": "^19.1.1",
    "socket.io": "^4.7.2",
    "socket.io-client": "^4.7.2",
    "terminal-kit": "^3.1.2",
    "viem": "^2.31.7",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "winston-transport": "^4.9.0",
    "zod": "^4.0.10"
  }
}